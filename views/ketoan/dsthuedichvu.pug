extends ./common

block main
  .p-4
    h2.text-center DANH SÁCH THUÊ DỊCH VỤ
    table.table.table-hover.table-sm
        thead.thead-light
          tr
            th Số phiếu
            th Mã dịch vụ
            th Số lượng
            th Giá dịch vụ
        tbody
          each phieu in dsDang
            tr
              td= phieu.SO_PHIEU
              td= phieu.MA_DICHVU
              td= phieu.SOLUONG
              td= phieu.GIA_DICHVU

    hr

    h2.text-center DANH SÁCH LƯU THUÊ DỊCH VỤ
    table.table.table-hover.table-sm
        thead.thead-light
          tr
            th Số phiếu
            th Mã dịch vụ
            th Số lượng
            th Giá dịch vụ
        tbody
          each phieu in dsDa
            tr
              td= phieu.SO_PHIEU
              td= phieu.MA_DICHVU
              td= phieu.SOLUONG
              td= phieu.GIA_DICHVU